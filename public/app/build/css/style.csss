/************************* custom checkboxes ************************/
.custom_checkbox {
  position: relative;
}

.custom_checkbox label {
  color: #b4bec7;
  display: inline-block;
  float: left;
  font-size: 20px;
  font-weight: 500;
  margin-bottom: 7px;
  padding-left: 28px;
  position: relative;
}

.custom_checkbox label span {
  color: #1e1e1d;
  font-size: 17px;
  line-height: normal;
}

.custom_checkbox input[type=checkbox] {
  display: none;
}

.left-sub-concept input[type="checkbox"]+label::before {
  content: "ï‚–";
  display: inline-block;
  font-family: FontAwesome;
  font-size: 18px;
  left: 0;
  letter-spacing: 10px;
  position: absolute;
  top: 5px;
}

.left-sub-concept input[type=checkbox]:checked+label:before {
  content: "\f046";
  letter-spacing: 5px;
  color: #375E97;
}

.custom_checkbox>input[type="checkbox"]+label::before {
  content: "\f196";
  display: inline-block;
  font-family: FontAwesome;
  font-size: 18px;
  left: 0;
  letter-spacing: 10px;
  position: absolute;
  top: 5px;
}

.custom_checkbox>input[type=checkbox]:checked+label:before {
  content: "\f147";
  letter-spacing: 5px;
  color: #375E97;
}

.custom_checkbox.form-group {
  margin-bottom: 0px;
  display: table;
}

.result-title {
  float: left;
  margin-bottom: 20px;
  width: 100%;
}

